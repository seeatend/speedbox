{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"env": {
		"browser": true
	},
	"rules": {
		"comma-dangle": 1,
		"indent": ["error", 2],
		"indent": 0,
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"object-shorthand": 0,
		"max-len": 0,
		"no-tabs": 0,
		"spaced-comment": 0,
		"spaced-line-comment": 0,
		"no-useless-escape": 0,
		"consistent-return": 0,
		"func-names": 0,
		"array-callback-return": 0,
		"prefer-arrow-callback": 0,
		"no-lonely-if": 0,
		"no-underscore-dangle": 0,
		"dot-notation": 0,
		"quote-props": 0,
		"arrow-parens": 0,
		"arrow-body-style": 0
	}
}
